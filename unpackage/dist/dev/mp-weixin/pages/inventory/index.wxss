.index_page .nav-btn {
  width: 80%;
  line-height: 120rpx;
  color: #333;
  border: 1px solid grey;
  margin: 50rpx auto;
  text-align: center;
  border-radius: 10rpx;
}
.index_page .nav-btn .icon-img {
  width: 50rpx;
  height: 50rpx;
  vertical-align: middle;
  margin-right: 25rpx;
}
.index_page .nav-btn:hover {
  background: #FE5BA4;
  color: #fff;
  border: 0;
}
