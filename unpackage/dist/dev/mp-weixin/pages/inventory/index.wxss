.index_page {
  width: 750rpx;
  height: 100vh;
  background-color: #F5F8FE;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  overflow-y: scroll;
  position: relative;
}
.index_page .canteen_nav {
  position: absolute;
  width: 750rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.index_page .canteen_nav image {
  width: 44rpx;
  height: 44rpx;
  margin-left: 24rpx;
  margin-right: 10rpx;
}
.index_page .canteen_nav view {
  font-size: 48rpx;
  color: #1A1A1A;
  line-height: 32rpx;
  margin: 28rpx 0 28rpx 32rpx;
}
.index_page .tip {
  /* 食堂管理，健康、高效、安全至上 */
  position: absolute;
  left: 46.66px;
  top: 105.67px;
  width: 300px;
  height: 22.97px;
  opacity: 1;
  font-family: ZhenyanGB;
  font-size: 20px;
  font-weight: 800;
  line-height: 140%;
  text-align: center;
  letter-spacing: 0em;
  text-align: center;
  font-variation-settings: "opsz" auto;
  color: #1DC36A;
  text-shadow: 0px 2px 2px rgba(24, 144, 255, 0.08);
}
.index_page .purchase {
  position: absolute;
  left: 20px;
  top: 172.94px;
  width: 336px;
  height: 78px;
  border-radius: 12px;
  opacity: 1;
  background: linear-gradient(180deg, #E6A733 0%, #E8AE39 100%);
  display: flex;
  align-items: center;
}
.index_page .purchase view > ._div {
  color: #FFFFFF;
  padding-left: 30rpx;
}
